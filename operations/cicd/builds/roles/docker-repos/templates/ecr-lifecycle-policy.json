{
  "rules": [
    {
      "rulePriority": 1,
      "description": "Expire CI images older than 14 days",
      "selection": {
        "tagStatus": "tagged",
        "tagPrefixList": ["{{build_ci_suffix}}"],
        "countType": "sinceImagePushed",
        "countUnit": "days",
        "countNumber": 14
      },
      "action": {
        "type": "expire"
      }
    },
    {
      "rulePriority": 2,
      "description": "Expire PR images older than 5 days",
      "selection": {
        "tagStatus": "tagged",
        "tagPrefixList": ["{{build_pr_suffix}}"],
        "countType": "sinceImagePushed",
        "countUnit": "days",
        "countNumber": 5
      },
      "action": {
        "type": "expire"
      }
    }
  ]
}